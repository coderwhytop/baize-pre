{
  "name": "baize-pre",
  "version": "0.2.9",
  "description": "A useful progressive front-end normalization tool.",
  "bin": {
    "baize": "dist/cli.cjs"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/baizeteam/baize-pre"
  },
  "keywords": [
    "front-end",
    "front-end format",
    "husky",
    "prettier",
    "husky",
    "basic",
    "vue",
    "react",
    "angular"
  ],
  "author": "kevin",
  "license": "ISC",
  "scripts": {
    "build": "rimraf dist && rollup -c rollup.config.mjs",
    "build:watch": "rollup -c rollup.config.mjs -w",
    "build:publish": "node scripts/bump-version.js && npm run build",
    "dev": "nodemon --watch dist --exec node dist/index.cjs",
    "start": "npm run build:watch",
    "prepublishOnly": "npm run build:publish",
    "format": "prettier --write .",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@octokit/rest": "^21.1.1",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "cross-env": "^7.0.3",
    "fs-extra": "^11.2.0",
    "inquirer": "^8.2.6",
    "readline-sync": "^1.4.10",
    "simple-git": "^3.27.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-json": "^6.1.0",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^9.1.1",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.18.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "nodemon": "^3.1.7",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "**/*.{js,ts,json}": "prettier --write ."
  }
}
